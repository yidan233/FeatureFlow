{
  "name": "canary-flags",
  "version": "1.0.0",
  "description": "A feature flag and canary release system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js start",
    "start:eval": "node dist/index.js start:eval",
    "start:control": "node dist/index.js start:control",
    "start:metrics": "node dist/index.js start:metrics",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/index.js start\"",
    "dev:eval": "ts-node src/index.ts start:eval",
    "dev:control": "ts-node src/index.ts start:control",
    "dev:metrics": "ts-node src/index.ts start:metrics",
    "cli": "ts-node src/cli/index.ts",
    "db:migrate": "ts-node src/index.ts migrate",
    "db:seed": "ts-node src/index.ts seed",
    "health": "ts-node src/index.ts health",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "./setup.sh",
    "postinstall": "npm run build"
  },
  "keywords": [
    "feature-flags",
    "canary",
    "deployment",
    "a/b-testing"
  ],
  "author": "Yidan Wang",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^11.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "inquirer": "^9.2.7",
    "ioredis": "^5.3.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.0",
    "prom-client": "^14.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "concurrently": "^8.2.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
